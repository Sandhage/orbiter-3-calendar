<h2>Hello World!</h2>
<p><%= @cash_donor.name %></p>
<p><%= @cash_donor.day_claimed %></p>
<% if user_signed_in? && current_user.admin? %>
    <p><%= link_to 'Edit This Cash Donor\'s Info', edit_cash_donor_path(@cash_donor) %></p>
    <p><%= link_to 'Delete', @cash_donor,
                       method: :delete,
                       data: { confirm: 'Are you sure about this?' }%></p>
<% end %>

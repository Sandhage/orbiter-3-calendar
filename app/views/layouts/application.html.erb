    <script type='text/javascript'>
      jQuery(document).ready(function(){
        artfully.widgets.cart();
        
        artfully.widgets.donation().display(3799,[]);
        artfully.widgets.cart().display();
      }); 
    </script> -->
  </head>
  <body>
    <header>
      <%= render 'layouts/navigation' %>
    </header>
